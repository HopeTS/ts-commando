{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "lib": ["DOM", "DOM.Iterable", "ES6", "ScriptHost"],
    "sourceMap": true,
    "outDir": "src/server",
    "baseUrl": "src/server",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src/server"],
  "exclude": ["node_modules", "src/client"]
}
